
@{
    ViewBag.Title = "MedExamFormStep4";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="row form-group">
        <div class="col-xs-12 col-lg-12">
            <ul class="nav nav-pills nav-justified thumbnail setup-panel">
                <li class="disabled">
                    <a href="#step-1">
                        <h4 class="list-group-item-heading">
                            Patient details
                        </h4>
                    </a>
                </li>
                <li class="disabled">
                    <a href="#step-2">
                        <h4 class="list-group-item-heading">
                            Medical Examiner Review
                        </h4>
                    </a>
                </li>
                <li class="disabled">
                    <a href="#step-3">
                        <h4 class="list-group-item-heading">
                            Medical Examiner Decision
                        </h4>
                    </a>
                </li>
                <li class="active">
                    <a href="#step-4">
                        <h4 class="list-group-item-heading">
                            SJR Assessment Triage
                        </h4>
                    </a>
                </li>
                <li class="disabled">
                    <a href="#step-5">
                        <h4 class="list-group-item-heading">
                            Other referrals
                        </h4>
                    </a>
                </li>
                <li class="disabled">
                    <a href="#step-6">
                        <h4 class="list-group-item-heading">
                            Positive Feedback
                        </h4>
                    </a>
                </li>
            </ul>
        </div>
    </div>
    <div class="row setup-content" id="step-4">
        <div class="col-xs-12 col-lg-12">
            <form action="/Home/MedExamFormStep4" method="post" id="frmMedExamFormStep4">
                <div class="col-md-12 well">
                    <div class="form-section">
                        <div class="lable_title">
                            <h2>SJR REVIEW TRIAGE</h2>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="row_bar checkbox-row">
                                    <input class="form-checkbox-input" type="checkbox" value="" id="cbPaediatricPatient" name="cbPaediatricPatient">
                                    <label class="label_part" for="defaultCheck1">Paediatric Patient  </label>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="row_bar checkbox-row">
                                    <input class="form-checkbox-input" type="checkbox" value="" id="cbLearningDisabilityPatient" name="cbLearningDisabilityPatient">
                                    <label class="label_part" for="defaultCheck1">Learning disability patient  </label>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="row_bar checkbox-row">
                                    <input class="form-checkbox-input" type="checkbox" value="" id="cbMentalIllnessPatient" name="cbMentalIllnessPatient">
                                    <label class="label_part" for="defaultCheck1">Severe mental health illness patient  </label>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="row_bar checkbox-row">
                                    <input class="form-checkbox-input" type="checkbox" value="" id="cbElectiveAdmission" name="cbElectiveAdmission">
                                    <label class="label_part" for="defaultCheck1">Elective admission   </label>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="row_bar checkbox-row">
                                    <input class="form-checkbox-input" type="checkbox" value="" id="cbNokConcernsDeath" name="cbNokConcernsDeath">
                                    <label class="label_part" for="defaultCheck1">NoK concerns about death    </label>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="row_bar checkbox-row">
                                    <input class="form-checkbox-input" type="checkbox" value="" id="cbOtherConcerns" name="cbOtherConcerns">
                                    <label class="label_part" for="defaultCheck1">Other concerns which warrant further review     </label>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="row_bar checkbox-row">
                                    <label class="label_part" for="defaultCheck1">Full SJR Required    </label>
                                    <input class="form-checkbox-input" type="checkbox" value="Yes" id="cbFullSJRRequiredYes" name="cbFullSJRRequiredYes">
                                    <label class="label_part" for="defaultCheck1">Yes   </label>
                                    <input class="form-checkbox-input" type="checkbox" value="No" id="cbFullSJRRequiredNo" name="cbFullSJRRequiredNo">
                                    <label class="label_part" for="defaultCheck1">No   </label>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="row_bar">
                                    <label class="label_part">Specialty to complete SJR Review </label>
                                </div>
                            </div>
                            <div class="col-md-9">
                                <div class="row_bar">
                                    <select class="form-control" id="ddlSJRReviewSpecialty" name="ddlSJRReviewSpecialty">
                                        <option value="-1">Select</option>
                                        <option value="M K S">M K S</option>
                                        <option value="P K P">P K P</option>
                                        <option value="R T O">R T O</option>
                                        <option value="T K T">T K T</option>
                                        <option value="D T O">D T O</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="pull-right">
                    <button id="activate-prev-1" name="BtnPrevious" class="btn btn-primary btn-lg">Previous</button>
                    <button id="activate-step-2" name="BtnNext" class="btn btn-primary btn-lg">Next</button>
                </div>
                </form>
        </div>
    </div>
   
</div>

<script>
    $(document).ready(function () {
    });
</script>

